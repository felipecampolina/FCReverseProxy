{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": false,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "graphTooltip": 0,
  "iteration": 1727260001002,
  "panels": [
    {
      "type": "stat",
      "title": "Total upstream requests",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "sum(upstream_requests_total{job=~\"$job\",instance=~\"$instance\",method=~\"$method\",status=~\"$status\"})",
          "legendFormat": "total",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "textMode": "auto", "colorMode": "value" }
    },
    {
      "type": "stat",
      "title": "Total upstream errors (4xx/5xx)",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
      "targets": [
        {
          "expr": "sum(upstream_requests_total{job=~\"$job\",instance=~\"$instance\",method=~\"$method\",status=~\"(4..|5..)\"})",
          "legendFormat": "errors",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "textMode": "auto", "colorMode": "value" }
    },
    {
      "type": "stat",
      "title": "Inflight (current)",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "sum(upstream_inflight{job=~\"$job\",instance=~\"$instance\"})",
          "legendFormat": "inflight",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "textMode": "auto", "colorMode": "value" }
    },
    {
      "type": "stat",
      "title": "Current RPS",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
      "targets": [
        {
          "expr": "sum(rate(upstream_requests_total{job=~\"$job\",instance=~\"$instance\",method=~\"$method\",status=~\"$status\"}[$__rate_interval]))",
          "legendFormat": "rps",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ],
      "fieldConfig": { "defaults": { "unit": "req/s" } },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "textMode": "auto", "colorMode": "value" }
    },
    {
      "type": "timeseries",
      "title": "Upstream RPS por status",
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 4 },
      "targets": [
        {
          "expr": "sum by (status) (rate(upstream_requests_total{job=~\"$job\",instance=~\"$instance\",method=~\"$method\",status=~\"$status\"}[$__rate_interval]))",
          "legendFormat": "{{status}}",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ],
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "single" } }
    },
    {
      "type": "timeseries",
      "title": "Upstream latência p50 / p90 / p99 (s)",
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 4 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le) (rate(upstream_request_duration_seconds_bucket{job=~\"$job\",instance=~\"$instance\",method=~\"$method\"}[5m])))",
          "legendFormat": "p50",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        },
        {
          "expr": "histogram_quantile(0.90, sum by (le) (rate(upstream_request_duration_seconds_bucket{job=~\"$job\",instance=~\"$instance\",method=~\"$method\"}[5m])))",
          "legendFormat": "p90",
          "refId": "B",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(upstream_request_duration_seconds_bucket{job=~\"$job\",instance=~\"$instance\",method=~\"$method\"}[5m])))",
          "legendFormat": "p99",
          "refId": "C",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" } },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "single" } }
    },
    {
      "type": "timeseries",
      "title": "Inflight (time series)",
      "gridPos": { "h": 7, "w": 24, "x": 0, "y": 11 },
      "targets": [
        {
          "expr": "sum(upstream_inflight{job=~\"$job\",instance=~\"$instance\"})",
          "legendFormat": "inflight",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ],
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "single" } }
    }
  ],
  "refresh": "10s",
  "schemaVersion": 39,
  "style": "dark",
  "tags": ["upstream", "go", "prometheus"],
  "templating": {
    "list": [
      {
        "name": "job",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(upstream_requests_total, job)",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "allValue": ".*"
      },
      {
        "name": "instance",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(upstream_requests_total{job=~\"$job\"}, instance)",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "allValue": ".*"
      },
      {
        "name": "method",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(upstream_requests_total, method)",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "allValue": ".*"
      },
      {
        "name": "status",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(upstream_requests_total, status)",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "allValue": ".*"
      }
    ]
  },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"],
    "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "7d"]
  },
  "timezone": "browser",
  "title": "Upstream — Overview",
  "uid": "upstream-overview",
  "version": 4,
  "weekStart": ""
}
