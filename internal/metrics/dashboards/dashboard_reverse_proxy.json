  "panels": [
    {
      "type": "stat",
      "title": "Total requests",
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "sum(proxy_requests_total{method=~\"$method\",status=~\"$status\",cache=~\"$cache\"})",
          "legendFormat": "total",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "textMode": "auto", "colorMode": "value" }
    },
    {
      "type": "stat",
      "title": "Total cache hits",
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 0 },
      "targets": [
        {
          "expr": "sum(proxy_requests_total{method=~\"$method\",status=~\"$status\",cache=\"HIT\"})",
          "legendFormat": "hits",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "textMode": "auto", "colorMode": "value" }
    },
    {
      "type": "stat",
      "title": "Total errors (4xx/5xx)",
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 0 },
      "targets": [
        {
          "expr": "sum(proxy_requests_total{method=~\"$method\",status=~\"(4..|5..)\",cache=~\"$cache\"})",
          "legendFormat": "errors",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "textMode": "auto", "colorMode": "value" }
    },
    {
      "type": "stat",
      "title": "Cache hit ratio (%)",
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "100 * sum(proxy_requests_total{method=~\"$method\",status=~\"$status\",cache=\"HIT\"}) / sum(proxy_requests_total{method=~\"$method\",status=~\"$status\",cache=~\"$cache\"})",
          "legendFormat": "hit %",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ],
      "fieldConfig": { "defaults": { "unit": "percent" } },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "textMode": "auto", "colorMode": "value" }
    },
    {
      "type": "stat",
      "title": "Current RPS",
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 0 },
      "targets": [
        {
          "expr": "sum(rate(proxy_requests_total{method=~\"$method\",status=~\"$status\",cache=~\"$cache\"}[$__rate_interval]))",
          "legendFormat": "rps",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ],
      "fieldConfig": { "defaults": { "unit": "req/s" } },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "textMode": "auto", "colorMode": "value" }
    },
    {
      "type": "timeseries",
      "title": "Proxy RPS por status",
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 4 },
      "targets": [
        {
          "expr": "sum by (status) (rate(proxy_requests_total{method=~\"$method\",status=~\"$status\",cache=~\"$cache\"}[$__rate_interval]))",
          "legendFormat": "{{status}}",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ],
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "single" } }
    },
